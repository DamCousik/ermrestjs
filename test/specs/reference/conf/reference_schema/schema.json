        "reference_values": {
            "comment": "Table to represent a testable reference object with markdown_pattern values",
            "kind": "table",
            "keys": [
                {
                    "comment": null,
                    "annotations": {},
                    "unique_columns": [
                        "id"
                    ]
                }
            ],
            "entityCount": 0,
            "foreign_keys": [],
            "table_name": "reference_values",
            "schema_name": "reference_schema",
            "column_definitions": [
                {
                    "name": "id",
                    "type": {
                        "typename": "text"
                    }
                },
                {
                    "name": "name",
                    "type": {
                        "typename": "text"
                    },
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "## {{name}}"
                            }
                        },
                        "tag:misd.isi.edu,2015:display": {
                            "show_nulls" : { "*" : false }
                        }
                    }
                },
                {
                    "name": "url",
                    "type": {
                        "typename": "text"
                    },
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "{{#name}}[link]({{url}}/{{name}}){{/name}}"
                            }
                        }
                    }
                },
                {
                    "name": "image",
                    "type": {
                        "typename": "text"
                    },
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "![image](http://example.com/{{id}}.png)"
                            }
                        }
                    }
                },
                {
                    "name": "image_with_size",
                    "type": {
                        "typename": "text"
                    },
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "![image with size]({{url}}/{{id}}.png){width=400 height=400}"
                            }
                        }
                    }
                },
                {
                    "name": "download_link",
                    "type": {
                        "typename": "text"
                    },
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "[download link]({{url}}){download}"
                            }
                        }
                    }
                },
                {
                    "name": "iframe",
                    "type": {
                        "typename": "text"
                    },
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : ":::iframe [{{name}} caption](http://example.com/iframe){width=300} \n:::"
                            }
                        }
                    }
                },
                {
                    "name": "some_markdown",
                    "type": {
                        "typename": "markdown"
                    },
                    "default": "**This is some markdown** with some `code` and a [link](http://www.example.com)"
                },
                {
                    "name": "some_markdown_with_pattern",
                    "type": {
                        "typename": "markdown"
                    },
                    "default": "**Name is :**",
                    "annotations": {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "{{{some_markdown_with_pattern}}} {{name}} \n{{{some_markdown}}}"
                            }
                        }
                    }
                },
                {
                    "name": "some_gene_sequence",
                    "type": {
                        "typename": "gene_sequence"
                    }
                },
                {
                    "name": "column_using_invisble_column",
                    "type" : {
                        "typename": "text"
                    },
                    "annotations" : {
                        "tag:isrd.isi.edu,2016:column-display" : {
                            "*" : {
                                "markdown_pattern" : "[{{some_invisible_column}}](http://example.com/{{some_invisible_column}})",
                                "show_nulls": "NA"
                            }
                        }
                    }
                },
                {
                    "name": "some_invisible_column",
                    "type" : {
                        "typename": "text"
                    }
                }
            ],
            "annotations": { 
                "tag:isrd.isi.edu,2016:visible-columns": {
                  "*": ["id", "name", "url", "image", "image_with_size", "download_link", "iframe", "some_markdown", "some_markdown_with_pattern", "some_gene_sequence", "column_using_invisble_column"]
                }
            }
        },
